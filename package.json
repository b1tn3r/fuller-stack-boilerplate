{
    "name": "fuller-stack-starter-app",
    "version": "1.0.0",
    "description": "Full Stack Redux application with routing capabilities and optimized performance with only a stand-in central app.",
    "repository": "b1tn3r/fuller-stack-starter-app",
    "private": true,
    "engines": {
        "node": "9.5.0",
        "npm": "5.6.0"
    },
    "scripts": {
        "clean": "rimraf public/",
        "test": "jest --coverage",
        "start": "heroku local",
        "dev": "better-npm-run build-dev",
        "dev-start": "better-npm-run start-dev",
        "prod": "better-npm-run prod",
        "prod-build": "better-npm-run build-prod",
        "prod-stop": "pm2 delete mainapp",
        "prod-list": "pm2 ls",
        "pretty-list": "pm2 prettylist",
        "prod-monitor": "pm2 monit",
        "prod-hot": "pm2 reload all",
        "max-processes": "pm2 ./server -i 0",
        "precommit": "npm run test",
        "prepush": "npm run test",
        "babel:start-dev": "nodemon ./server --exec babel-node --presets es2015,stage-2,react --ignore public/",
        "babel:build-prod ": "npm run clean && babel src -d lib/ --ignore .test.js",
        "babel:start-prod": "pm2 start ./lib/server && pm2 log",
        "heroku:prod-start": "heroku local",
        "heroku:post-build": "npm run prod-build"
    },
    "betterScripts": {
        "build-dev": {
            "command": "webpack -wd --progress",
            "env": {
                "NODE_PATH": ".",
                "NODE_ENV": "development",
                "PORT": 3000,
                "APIPORT": 3030
            }
        },
        "start-dev": {
            "command": "nodemon ./server/index.js --ignore public/",
            "env": {
                "HOST": "127.0.0.1",
                "NODE_PATH": ".",
                "NODE_ENV": "development",
                "PORT": 3000,
                "APIPORT": 3030
            }
        },
        "build-prod": {
            "command": "webpack --progress",
            "env": {
                "NODE_PATH": ".",
                "NODE_ENV": "production",
                "PORT": 3000,
                "APIPORT": 3030
            }
        },
        "prod": {
            "command": "webpack --progress && pm2 start --name mainapp ./server/index.js && pm2 log",
            "env": {
                "NODE_PATH": ".",
                "NODE_ENV": "production",
                "PORT": 3000,
                "APIPORT": 3030
            }
        }
    },
    "dependencies": {
        "axios": "^0.17.1",
        "babel-polyfill": "^6.26.0",
        "body-parser": "~1.18.2",
        "compression": "^1.7.2",
        "cookie-parser": "~1.4.3",
        "debug": "~2.6.9",
        "express": "~4.15.5",
        "history": "^4.7.2",
        "http-proxy": "^1.16.2",
        "immutable": "^3.8.2",
        "isomorphic-fetch": "^2.2.1",
        "leaflet": "^1.3.1",
        "material-ui": "^0.20.0",
        "mongoose": "^5.0.8",
        "morgan": "~1.9.0",
        "pm2": "^2.10.1",
        "pug": "2.0.0-beta11",
        "react": "^16.2.0",
        "react-dom": "^16.2.0",
        "react-form": "^3.3.0",
        "react-helmet": "^5.2.0",
        "react-leaflet": "^1.9.0",
        "react-redux": "^5.0.7",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.9",
        "redux": "^3.7.2",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.2.0",
        "reselect": "^3.0.1",
        "serialize-javascript": "^1.4.0",
        "serve-favicon": "^2.4.5",
        "socket.io": "^2.0.4",
        "socket.io-client": "^2.0.4",
        "source-map-support": "^0.5.3",
        "styled-components": "^3.1.6",
        "validator": "^9.4.1",
        "window-or-global": "^1.0.1"
    },
    "devDependencies": {
        "autoprefixer": "^8.1.0",
        "axios-mock-adapter": "^1.14.1",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-jest": "^22.4.1",
        "babel-loader": "^7.1.2",
        "babel-plugin-add-module-exports": "^0.2.1",
        "babel-plugin-react-html-attrs": "^2.1.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-react-constant-elements": "^6.23.0",
        "babel-plugin-transform-react-inline-elements": "^6.22.0",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "better-npm-run": "^0.1.0",
        "bootstrap": "^4.0.0",
        "bootstrap-material-design": "^4.1.1",
        "bootswatch": "^4.0.0",
        "coveralls": "^3.0.0",
        "css-loader": "^0.28.10",
        "expose-loader": "^0.7.5",
        "extract-loader": "^1.0.2",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^1.1.11",
        "husky": "^0.14.3",
        "image-webpack-loader": "^4.1.0",
        "jest": "^22.4.2",
        "jquery": "^3.3.1",
        "json-loader": "^0.5.7",
        "node-sass": "^4.7.2",
        "node-sass-middleware": "^0.11.0",
        "nodemon": "^1.17.1",
        "popper.js": "^1.14.1",
        "postcss-loader": "^2.1.0",
        "precss": "^3.1.2",
        "prop-types": "^15.6.0",
        "react-hot-loader": "^4.0.0",
        "redux-mock-store": "^1.5.1",
        "rimraf": "^2.6.2",
        "sass-loader": "^6.0.7",
        "style-loader": "^0.20.2",
        "url-loader": "^0.6.2",
        "webpack": "^3.11.0",
        "webpack-dev-middleware": "^2.0.6",
        "webpack-dev-server": "^2.11.1",
        "webpack-hot-middleware": "^2.21.2",
        "webpack-node-externals": "^1.6.0"
    },
    "babel": {
        "presets": [
            "es2015",
            "stage-2",
            "react"
        ],
        "plugins": [
            "react-hot-loader/babel",
            "react-html-attrs",
            "transform-class-properties",
            "transform-decorators-legacy"
        ],
        "env": {
            "jest": true,
            "production": {
                "plugins": [
                    "transform-react-constant-elements",
                    "transform-react-inline-elements",
                    "transform-react-remove-prop-types"
                ]
            }
        }
    },
    "author": "Titan Global Tech, LLC",
    "license": "MIT"
}
